@using TimeSheetMobileWeb.Models
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=320, height=device-height, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=0"/>
    <meta name="MobileOptimized" content="320"/> 
    <link href="@Url.Content("~/Content/Site-1.0.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/jquery-ui-1.8.17.custom.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/site.jquery.mobile-1.0.css.min.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/jquery.mobile.structure-1.1.0.min.css")" rel="stylesheet" type="text/css" />
    
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script type="text/javascript">
        var TSM_Checked = "@SiteResources.Checked";
        var TSM_UnChecked = "@SiteResources.UnChecked";
        $(document).bind('mobileinit', function () {
            $.mobile.page.prototype.options.keepNative = ":not(.mobilegadget)";
            $.mobile.metaViewportContent = false;
            $.mobile.loadingMessage = '@SiteResources.Loading';
            $.mobile.loadingMessageTheme = 'a';
            $.mobile.loadingMessageTextVisible = true;
        });
    </script>
    <script src="@Url.Content("~/Scripts/jquery.mobile-1.1.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.17.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/globalize.min.js")" type='text/javascript' ></script>
    @Html.GlobalizationScript()
    @Html.JQueryDatePickerGlobalizationScript()
    <script src="@Url.Content("~/Scripts/MVCControlToolkit.Controls.Core-1.8.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MVCControlToolkit.Controls.Items-1.8.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cookies.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SiteScript-1.0.js")" type="text/javascript"></script>
    
    @RenderSection("head", false)
</head>

<body>
    
    @RenderBody()
</body>
</html>
