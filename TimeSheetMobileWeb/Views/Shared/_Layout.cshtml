@using TimeSheetMobileWeb.Models
<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title</title>
    <meta name="viewport" content="width=320, height=device-height, initial-scale=1, maximum-scale=1, minimum-scale=1, user-scalable=0"/>
    <meta name="MobileOptimized" content="320"/> 
    <link href="@Url.Content("~/Content/jquery-ui-1.8.17.custom.css")" rel="stylesheet" type="text/css" />
     <link href="@Url.Content("~/Content/BP/css/main.css")" rel="stylesheet" type="text/css" />

    
    <link href="favicon.ico" type="image/x-icon" rel="shortcut icon"></link>

<meta content="width=device-width, user-scalable=no" name="viewport"></meta>

<meta content="IE=100" http-equiv="X-UA-Compatible"></meta>

<script src="@Url.Content("~/Scripts/modernizr.2.7.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-1.7.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/main.js")" type="text/javascript"></script>
     
    
    <script type="text/javascript">
        var TSM_Checked = "@SiteResources.Checked";
        var TSM_UnChecked = "@SiteResources.UnChecked";
        $(document).bind('mobileinit', function () {
            $.mobile.page.prototype.options.keepNative = ":not(.mobilegadget)";
            $.mobile.metaViewportContent = false;
            $.mobile.loadingMessage = '';
        });
    </script>
    <script src="@Url.Content("~/Scripts/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery-ui-1.8.17.custom.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/globalize.min.js")" type='text/javascript' ></script>
    
    @Html.GlobalizationScript()
    <script src="@Url.Content("~/Scripts/MVCControlToolkit.Controls.Core-1.8.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/MVCControlToolkit.Controls.Items-1.8.0.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/cookies.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/SiteScript-1.0.js")" type="text/javascript"></script>
    
    
    
    @RenderSection("head", false)
</head>

<body class="tms time-details">
<div class="preloader" id="preloader"></div>
 <div id="modalOveray" class="modalOveray"></div>
    <header>
        <a class="logo leaveapp" title="tsm" href="#" class ='leaveapp' data-target='@Url.Action("Index", "Home")' data-leave-application='@SiteResources.LeaveApplication'></a>
    </header>
    @RenderBody()
</body>
</html>
