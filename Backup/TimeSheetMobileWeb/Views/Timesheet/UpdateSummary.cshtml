@model TimeSheetMobileWeb.Models.UpdateTimesheetsView
@using TimeSheetMobileWeb.Models
@using TimeSheetIBusiness
<div id="tsSummary" class="modal">
        <div class="modal-content">
                        <p><span class=' updateerrormessage-d'></span></p>
                        <p>
                         <span class=''>@SiteResources.TimesheetNameLabel</span><span class='currname-d'></span><br />
                         <span class=''>@SiteResources.TimesheetStatusLabel</span><span class='currstatus-d'></span><br />
                         <span class=''>@SiteResources.TotalActualWorkLabel</span><span class='currtotalact-d' data-format="@Html.ClientFormatting(m => m.HeaderInfos.TotalActualWork)"></span><br />
                         <span class=''>@SiteResources.TotalOvertimeWorkLabel</span><span class='currtotalovertime-d' data-format="@Html.ClientFormatting(m => m.HeaderInfos.TotalOverTimeWork)"></span><br />
                         <span class=''>@SiteResources.TotalNonBillableLabel</span><span class='currtotalnonbill-d' data-format="@Html.ClientFormatting(m => m.HeaderInfos.TotalNonBillable)"></span><br />
                         <span class=''>@SiteResources.TotalOvertimeNonBillableLabel</span><span class='currtotalovertimenonbill-d' data-format="@Html.ClientFormatting(m => m.HeaderInfos.TotalNonBillableOvertime)"></span><br />
                         <span class=''>@SiteResources.TimesheetCommentsLabel</span><span class='currcomments-d'></span>
                        </p>

            <p class="buttons-container">
            <input id="btnTimesheetDone" type="button" value="@SiteResources.OK" class='main-submit cancel btn' data-role="dismiss" data-target='#tsSummary' 
            data-callback='GotoUrl' data-row-from="@Url.Action("Index","Timesheet")"/>  
            </p> 
    </div>
</div>

