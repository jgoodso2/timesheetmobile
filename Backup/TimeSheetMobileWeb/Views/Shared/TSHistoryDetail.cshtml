@using MVCControlsToolkit.Controls
@using MVCControlsToolkit.Core


@model TimeSheetMobileWeb.Models.TimeSheetView
    <td class="editor-label gridcelllink" onclick='GotoUrl("@Url.Action(
    "Index",
"Timesheet", new { speriod = @Model.Period.Replace("(","-").Replace(")","-") }, HttpContext.Current.Request.Url.Scheme)");'>
     
                                            @Model.Period
                                            
      

    </td>
    <td class="editor-label gridcell" onclick='GotoUrl("@Url.Action(
    "Index",
"Timesheet", new { speriod = @Model.Period.Replace("(", "-").Replace(")", "-") }, HttpContext.Current.Request.Url.Scheme)");'>
        @Html.DisplayField(m => m.Status)
    </td>
    <td class="editor-label gridcell" onclick='GotoUrl("@Url.Action(
    "Index",
"Timesheet", new { speriod = @Model.Period.Replace("(", "-").Replace(")", "-") }, HttpContext.Current.Request.Url.Scheme)");'>
        @Html.DisplayField(m => m.Hours)
    </td>
